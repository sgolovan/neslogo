\ProvidesPackage{neslogo}[2024/04/07 The New Economic School logo]

\RequirePackage{tikz}
\RequirePackage{xparse}
\RequirePackage{kvoptions}
\RequirePackage{ifluatex}
\RequirePackage{ifxetex}
\RequirePackage{ifthen}
\RequirePackage{xkeyval}

\RequirePackage{neslogoold}
\RequirePackage{neslogonew}

\NewDocumentCommand\neslogo{O{}}{%
  \setkeys{neslogo}{#1}%
  \ifKV@neslogo@old
    \neslogoold[#1]
  \else
    \neslogonew[#1]
  \fi
}

